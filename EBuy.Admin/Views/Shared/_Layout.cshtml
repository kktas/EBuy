    <link href="css\admin.css" rel="stylesheet">

    <!-- Page Specific css here-->
    @RenderSection("styles", false)
    <!-- Page Specific css here-->
</head>

<body>
    <!-- SIDEBAR -->
    @await Html.PartialAsync("~/Views/Partials/Sidebar.cshtml")
    <!-- SIDEBAR -->

    <div class="wrapper min-vh-100 ">

        <!--HEADER-->
        @await Html.PartialAsync("~/Views/Partials/Header.cshtml")
        <!--HEADER-->

        <!-- BREADCRUMB -->
        @await Html.PartialAsync("~/Views/Partials/Breadcrumb.cshtml", Model.View.Breadcrumbs)
        <!-- BREADCRUMB -->

        <!--MAIN-->
        <div class="card mb-0 rounded-0 h-100">

            @RenderBody()
        </div>

        <!--MAIN-->
    </div>
