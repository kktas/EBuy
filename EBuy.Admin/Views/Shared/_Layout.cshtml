@model IPage
@{
    var pageIsIndex = Model.GetType().IsGenericType && Model.GetType().GetGenericTypeDefinition() == typeof(PageIndex<>);
    var pageIsCreate = Model.GetType().IsGenericType && Model.GetType().GetGenericTypeDefinition() == typeof(PageCreate);
    var pageIsEdit = Model.GetType().IsGenericType && Model.GetType().GetGenericTypeDefinition() == typeof(PageEdit<>);
}

